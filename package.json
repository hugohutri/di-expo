{
  "name": "expo-dev",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android:old": "expo start --android --tunnel",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "android": "npx expo prebuild && npx expo run:android",
    "i18n:extract": "lingui extract --clean",
    "i18n:compile": "lingui compile"
  },
  "dependencies": {
    "@expo/webpack-config": "^0.17.0",
    "@lingui/macro": "3.14.0",
    "@lingui/react": "3.14.0",
    "@react-native-community/netinfo": "9.3.0",
    "expo": "~47.0.8",
    "expo-cli": "^6.0.6",
    "expo-constants": "~13.2.4",
    "expo-device": "~4.3.0",
    "expo-localization": "~13.1.0",
    "expo-network": "~4.3.0",
    "expo-status-bar": "~1.4.0",
    "i18n-js": "^4.1.1",
    "luxon": "^3.1.1",
    "make-plural": "^7.2.0",
    "patch-package": "^6.5.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-freeze": "1.0.3",
    "react-native": "0.70.5",
    "react-native-localize": "^2.2.4",
    "react-native-web": "~0.18.7",
    "expo-splash-screen": "~0.17.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/runtime": "7.12.5",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "metro-react-native-babel-preset": "0.70.3",
    "@lingui/cli": "3.14.0",
    "@types/luxon": "^3.1.0",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "private": true,
  "lingui": {
    "locales": [
      "en",
      "fi"
    ],
    "sourceLocale": "en",
    "catalogs": [
      {
        "path": "src/locales/{locale}/messages",
        "include": [
          "src"
        ]
      }
    ],
    "format": "po"
  }
}
